{
  "name": "car-marketplace-philippines-api",
  "version": "1.0.0",
  "description": "A comprehensive Node.js TypeScript server for a car marketplace specifically designed for the Philippines market",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migration:create": "node scripts/create-migration.js",
    "migration:run": "node scripts/run-migrations.js",
    "seed": "node scripts/seed-database.js",
    "queue:worker": "node dist/workers/queue-worker.js",
    "scheduler:start": "node dist/workers/scheduler.js",
    "clean": "rm -rf dist",
    "prepare": "husky install"
  },
  "keywords": [
    "car-marketplace",
    "philippines",
    "nodejs",
    "typescript",
    "redis",
    "mysql",
    "socket.io",
    "real-time",
    "fraud-detection",
    "analytics"
  ],
  "author": "Car Marketplace Philippines Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^5.0.0",
    "mysql2": "^3.6.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "@socket.io/redis-adapter": "^8.2.1",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "joi": "^17.9.0",
    "multer": "^1.4.4",
    "sharp": "^0.32.0",
    "bull": "^4.11.0",
    "winston": "^3.10.0",
    "nodemailer": "^6.9.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.8.0",
    "express-validator": "^7.0.0",
    "moment-timezone": "^0.5.43",
    "geolib": "^3.3.4",
    "image-thumbnail": "^1.0.15",
    "aws-sdk": "^2.1400.0",
    "dotenv": "^16.3.0",
    "node-cron": "^3.0.2",
    "uuid": "^9.0.0",
    "crypto": "^1.0.1",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "util": "^0.12.5",
    "sanitize-html": "^2.11.0",
    "validator": "^13.9.0",
    "twilio": "^4.14.0",
    "firebase-admin": "^11.10.1",
    "google-maps": "^4.3.3",
    "paymongo": "^1.1.0",
    "qrcode": "^1.5.3",
    "pdf-lib": "^1.17.1",
    "xlsx": "^0.18.5",
    "csv-parser": "^3.0.0",
    "archiver": "^5.3.1",
    "mime-types": "^2.1.35",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "xss": "^1.0.14",
    "rate-limiter-flexible": "^2.4.2",
    "node-cache": "^5.1.2",
    "lodash": "^4.17.21",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^20.7.0",
    "dayjs": "^1.11.9",
    "zod": "^3.21.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^20.4.0",
    "@types/express": "^4.17.17",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/joi": "^17.2.3",
    "@types/multer": "^1.4.7",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/validator": "^13.7.17",
    "@types/uuid": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/node-cron": "^3.0.8",
    "@types/fs-extra": "^11.0.1",
    "@types/sanitize-html": "^2.9.0",
    "@types/qrcode": "^1.5.1",
    "@types/archiver": "^5.3.2",
    "@types/mime-types": "^2.1.1",
    "@types/express-session": "^1.17.7",
    "@types/cheerio": "^0.22.31",
    "@types/puppeteer": "^7.0.4",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "prettier": "^3.0.0",
    "jest": "^29.6.0",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "cross-env": "^7.0.3",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/car-marketplace-philippines-api.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/car-marketplace-philippines-api/issues"
  },
  "homepage": "https://github.com/your-org/car-marketplace-philippines-api#readme",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types/**/*"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended"
    ],
    "env": {
      "node": true,
      "es2022": true
    },
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/explicit-function-return-type": "off"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  }
}
